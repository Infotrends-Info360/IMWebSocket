<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE mapper
    PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
    "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="cfg_casestatus">

  <resultMap type="com.Info360.bean.Cfg_CaseStatus" id="cfg_casestatusResult">
  
    <id     column="DBID" 		jdbcType="INTEGER" property="dbid"/>
    <result column="STATUSNAME" jdbcType="VARCHAR" property="statusname"/>
   
   
    
</resultMap>

<select id="Select_IXN_cfg_casestatus" parameterType="com.Info360.bean.Cfg_CaseStatus"  resultMap="cfg_casestatusResult">
 Select DBID,statusname from tblCfg_CaseStatus where DBID=#{dbid}
</select>


</mapper>
