<select id="Selcet_interaction" parameterType="com.Info360.bean.Interaction"  resultMap="interactionResult">
<!-- Select CONTACTID,IXNID,AGENTID,STARTDATE,ENDDATE,STATUS,TYPEID,ENTITYTYPEID,SUBTYPEID  -->
<!-- SUBJECT,TEXT,STRUCTUREDTEXT,STRUCTUREDMIMETYPE,THECOMMENT,STOPPEDREASON,ACTIVITYCODE  -->
<!--  from tblInteraction  -->
 
select top 10 DBID,CONTACTID,IXNID,AGENTID,STARTDATE,ENDDATE,STATUS,TYPEID,ENTITYTYPEID,SUBTYPEID,SUBJECT,TEXT,STRUCTUREDTEXT,STRUCTUREDMIMETYPE,THECOMMENT from 
( select row_number() over(order by dbid asc) as rownumber,DBID,CONTACTID,IXNID,AGENTID,STARTDATE,ENDDATE,STATUS,TYPEID,ENTITYTYPEID,SUBTYPEID,SUBJECT,TEXT,STRUCTUREDTEXT,STRUCTUREDMIMETYPE,THECOMMENT from (select DBID,CONTACTID,IXNID,AGENTID,STARTDATE,ENDDATE,STATUS,TYPEID,ENTITYTYPEID,SUBTYPEID,SUBJECT,TEXT,STRUCTUREDTEXT,STRUCTUREDMIMETYPE,THECOMMENT FROM tblInteraction where 1=1


<if test="agentid!=&quot;&quot;">
 	and 
 		AGENTID =#{agentid}
</if>
<if test="startdate!=&quot;&quot;">
	and
 		<![CDATA[ Convert(DateTime,StartDate) >=Convert(DateTime,#{startdate})]]>
</if>
<if test="enddate!=&quot;&quot;">
    and  
   		<![CDATA[ Convert(DateTime,EndDate) <= Convert(DateTime,DATEADD(day,1,#{enddate}))  ]]>
</if>
) as comImg)
as numComImg where rownumber>#{page}
</select>

<select id="SelcetAll_interaction" parameterType="com.Info360.bean.Interaction"  resultMap="interactionResult">
Select DBID,CONTACTID,IXNID,AGENTID,STARTDATE,ENDDATE,STATUS,TYPEID,ENTITYTYPEID,SUBTYPEID
SUBJECT,TEXT,STRUCTUREDTEXT,STRUCTUREDMIMETYPE,THECOMMENT
from tblInteraction where 1=1

<if test="agentid!=&quot;&quot;">
 	and 
 		AGENTID =#{agentid}
</if>
<if test="startdate!=&quot;&quot;">
	and
 		<![CDATA[ Convert(DateTime,StartDate) >=Convert(DateTime,#{startdate})]]>
</if>
<if test="enddate!=&quot;&quot;">
    and  
   		<![CDATA[ Convert(DateTime,EndDate) <= Convert(DateTime,DATEADD(day,1,#{enddate}))  ]]>
</if>
</select>



	<insert id="Insert_Interaction" parameterType="com.Info360.bean.Interaction">
  	
		insert into TBLINTERACTION 
 	<foreach item="value" index="key" collection="interactionlist"
     open="(" separator="," close=")">
        ${key}
   	</foreach>
      values 
    <foreach item="value" index="key" collection="interactionlist"
     open="(" separator="," close=")">
        #{value}
   	</foreach>
   	 
  	</insert>
  	
  	<update id="Update_Interaction_comment" parameterType="com.Info360.bean.Interaction">
    
    update  TBLINTERACTION  set THECOMMENT=#{thecomment}
    
<!-- 	<foreach item="value" index="key" collection="userdata" -->
<!--      open="," separator="," close=""> -->
<!--         ${key}=#{value} -->
<!--    	</foreach> -->
    
    where IXNID=#{ixnid}
  
  </update>
  
</mapper>
